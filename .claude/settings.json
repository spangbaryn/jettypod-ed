{
  "hooks": {
    "PreToolUse": [
      {
        "matcher": "Edit",
        "hooks": [
          {
            "type": "command",
            "command": ".devpod/hooks/protect-claude-md.js"
          }
        ]
      },
      {
        "matcher": "Write",
